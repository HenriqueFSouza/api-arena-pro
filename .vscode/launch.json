{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Debug Nest Framework",
        "runtimeExecutable": "pnpm",
        "runtimeArgs": [
          "start:debug"
        ],
        "sourceMaps": true,
        "envFile": "${workspaceFolder}/.env",
        "skipFiles": [
          "<node_internals>/**"
        ],
        "outFiles": [
            "${workspaceFolder}/dist/**/*.js"
        ],
        "console": "integratedTerminal"
      }
    ]
}